  "metadata": {
    "name": ["p.bname > a"],
    "author": ["p.author > a"], 
    "cover": ["a > img@attr->src"], 
    "summary": [".data > p:nth-child(3)"], 
    "category": ["p.tags > span > a"],
    "status": ["span.finished"], 
    "update": [".last-chapter@js->${this}.match(/(.*) 更/)[1]"], 
    "lastChapter": ["p.last > a"], 

    "link": ["p.bname > a@attr->href"], 
    "catalog": [".catelog > div > a@attr->href"] 
  },
  "catalog": {
    "list": ".catalog > ul > li",
    "orderBy": 0, 
    "chapter": {
      "name": "a",  
      "link": "a@attr->href"   
    }
  },
  "content": {
    "text": ".section-main-con" 
  },

  "search": {
    "link": "http://www.itangyuan.com/search/book/${key}.html", 
    "list": ".book-list > div" 
  },
